{
  "version": 2,
  "buildCommand": "pnpm run build && pnpm run build-storybook && cp registry.json dist/ && cp -r storybook-static dist/",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/storybook",
      "dest": "/storybook-static/index.html"
    },
    {
      "src": "/storybook/(.*)",
      "dest": "/storybook-static/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}