{
  "version": 2,
  "buildCommand": "pnpm run build && pnpm run build-storybook",
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "storybook-static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/storybook/(.*)",
      "dest": "/storybook-static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}