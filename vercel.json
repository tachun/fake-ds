{
  "version": 2,
  "buildCommand": "pnpm run build && pnpm run build-storybook && cp registry.json dist/",
  "outputDirectory": "dist",
  "publicSourceDirectories": [
    "storybook-static"
  ],
  "rewrites": [
    {
      "source": "/storybook",
      "destination": "/storybook-static/index.html"
    },
    {
      "source": "/storybook/:path*",
      "destination": "/storybook-static/:path*"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}