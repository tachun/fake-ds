{
  "version": 2,
  "buildCommand": "pnpm run build && pnpm run build-storybook && cp registry.json dist/ && cp -r storybook-static dist/storybook",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/storybook",
      "dest": "/storybook/index.html"
    },
    {
      "src": "/storybook/(.*)",
      "dest": "/storybook/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}