{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "pnpm run build-registry && mkdir -p dist/r && cp -r public/r/* dist/r/ && pnpm run build && pnpm run build-storybook && cp -r storybook-static/* dist/storybook/",
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/storybook",
      "dest": "/storybook/index.html"
    },
    {
      "src": "/storybook/index.html",
      "dest": "/storybook/index.html"
    },
    {
      "src": "/storybook/(.*)",
      "dest": "/storybook/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}